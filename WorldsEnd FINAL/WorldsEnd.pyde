def worldeaterphase():
    global global_turn_count
    global currentphase
    global turn_count
    if 0 <= global_turn_count <= 10:
        currentphase = 1
    elif 11 <= global_turn_count <= 30:
        currentphase = 2
    elif 31 <= global_turn_count <= 50:
        currentphase = 3
    else:
        currentphase = 4
    fill(255,255,255)
    rect(1150,100,150,30)
